{
    "name": "luthien-mc",
    "version": "0.0.7",
    "main": "main.js",
    "scripts": {
        "start": "electron ."
    },
    "build": {
        "appId": "com.candiedapple.luthienminecraft",
        "publish": [
            {
                "provider": "github",
                "owner": "candied-apple",
                "repo": "Luthien-Minecraft-Launcher"
            }
        ],
        "mac": {
            "category": "public.game",
            "target": "dmg",
            "icon": "build/icon.icns"
        },
        "linux": {
            "target": "AppImage",
            "category": "Game",
            "icon": "build/icon.png"
        },
        "win": {
            "target": "nsis",
            "icon": "build/icon.ico"
        },
        "files": [
            "build/**/*",
            "index.html",
            "main.js",
            "renderer.js",
            "styles.css"
        ],
        "directories": {
            "buildResources": "build"
        }
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "bcrypt": "^5.1.1",
        "bcryptjs": "^2.4.3",
        "dotenv": "^16.4.5",
        "electron-updater": "^6.2.1",
        "minecraft-launcher-core": "^3.18.1",
        "mysql": "^2.18.1",
        "tomate-loaders": "^2.0.2"
    },
    "devDependencies": {
        "electron": "^31.3.1",
        "electron-builder": "^24.13.3"
    }
}
